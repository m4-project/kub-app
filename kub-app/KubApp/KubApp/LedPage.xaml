<Page
    x:Class="KubApp.LedPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:KubApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button Name="changeMood" Content="Verander Stemming" HorizontalAlignment="Left" Margin="150,200,0,0" VerticalAlignment="Top" Click="Stemming_Click"/>
        <Rectangle x:Name="currentColor" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="100" Margin="212,315,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="LinkMainPage" Content="Main Page" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="LinkMainPage_Click"/>

    </Grid>
</Page>
